%rezolutie temporara de 2 ms
t=0:0.002:4; %afisarea a 2 perioade
f=0.25; %frecventa
w=2*pi*f; %pulsatia
amplit=1.5; %amplitudinea

s=1.5* abs(sin(w*t));
subplot(3,1,1) %imparte fereastra de tip fig intr-o matrice 3x1 de mici subgrafice se selecteaza subplotul 1 ca grafic curent
plot(t,s),grid,xlabel('t,[s]'),ylabel('A,[V]');

%plot produce graficul lui s versus t
%xlabel si ylabel-completarea graficului cu etichete pe axe, legenda, text

%rezolutie temporara de 20 ms
t=0:0.02:4; %afisarea a 2 perioade
f=0.25; %frecventa
w=2*pi*f; %pulsatia
amplit=1.5; %amplitudinea

s=1.5* abs(sin(w*t));
subplot(3,1,2) %imparte fereastra de tip fig intr-o matrice 3x1 de mici subgrafice se selecteaza subplotul 1 ca grafic curent
plot(t,s),grid,xlabel('t,[s]'),ylabel('A,[V]');

%plot produce graficul lui s versus t
%xlabel si ylabel-completarea graficului cu etichete pe axe, legenda, text

%rezolutie temporara de 200 ms
t=0:0.2:4; %afisarea a 2 perioade
f=0.25; %frecventa
w=2*pi*f; %pulsatia
amplit=1.5; %amplitudinea

s=1.5* abs(sin(w*t));
subplot(3,1,3) %imparte fereastra de tip fig intr-o matrice 3x1 de mici subgrafice se selecteaza subplotul 1 ca grafic curent
plot(t,s),grid,xlabel('t,[s]'),ylabel('A,[V]');

%plot produce graficul lui s versus t
%xlabel si ylabel-completarea graficului cu etichete pe axe, legenda, text
